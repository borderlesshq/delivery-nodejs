  "description": "A Nodejs SDK for BorderlessHQ.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "start": "node dist/src/index.js",
    "start:dev": "nodemon dist/src/index.js",
    "clean": "rimraf coverage build tmp",
